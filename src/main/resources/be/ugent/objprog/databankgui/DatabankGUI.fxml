<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<VBox
        spacing="20.0" xmlns:fx="http://javafx.com/fxml"
        fx:controller="be.ugent.objprog.databankgui.DatabankGUIController"
        fx:id="rootVBOX"
    >
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>

    <HBox spacing="10.0" xmlns:fx="http://javafx.com/fxml">
        <Label text="Naam begint met:" styleClass="HeadLabel"/>
        <TextField prefWidth="300" fx:id="inputField"/>
        <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" fx:id="searchButton">
            <graphic>
                <ImageView fitHeight="10.0" fitWidth="10.0" pickOnBounds="true" preserveRatio="true">
                    <Image url="@icons/search.png"/>
                </ImageView>
            </graphic>
        </Button>

        <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" fx:id="addButton">
            <graphic>
                <ImageView fitHeight="10.0" fitWidth="10.0" pickOnBounds="true" preserveRatio="true">
                    <Image url="@icons/plus.png"/>
                </ImageView>
            </graphic>
        </Button>
    </HBox>

</VBox>